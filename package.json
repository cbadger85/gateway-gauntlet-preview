{
  "name": "gateway-gauntlet",
  "version": "1.0.0",
  "description": "Event page for the 2019 Gateway Gauntlet",
  "main": "index.html",
  "scripts": {
    "start": "npm run build && node ./server",
    "dev": "concurrently \"npm:serve\" \"npm:webpack-dev\" --kill-others --names \"SERVER,WEBPACK\" -c \"bgBlue.bold,bgGreen.red.italic\"",
    "watch": "nodemon ./server --ignore public/",
    "webpack-dev": "webpack-dev-server --mode development --open --disable-host-check",
    "serve": "netlify-lambda serve ./lambda -t 100",
    "build:functions": "netlify-lambda build ./lambda",
    "build:prod": "webpack --mode production",
    "build": "npm run build:prod && npm run build:functions"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbadger85/gateway-gauntlet.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cbadger85/gateway-gauntlet/issues"
  },
  "homepage": "https://github.com/cbadger85/gateway-gauntlet#readme",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "querystring": "^0.2.0",
    "serverless-http": "^1.9.0",
    "stripe": "^6.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.8",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^3.0.1",
    "html-webpack-inline-svg-plugin": "^1.2.4",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webpack": "^4.0.1",
    "imagemin-webpack-plugin": "^2.3.0",
    "mini-css-extract-plugin": "^0.5.0",
    "netlify-lambda": "^1.4.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-md5-hash": "0.0.6",
    "webpack-node-externals": "^1.7.2"
  }
}
